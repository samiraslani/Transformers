   "source": [
    "# Create the positional embeddings: \n",
    "position_enc_training = pos_emb(X_trainmod.shape[1],X_trainmod.shape[2])\n",
    "print(position_enc_training.shape)\n",
    "position_enc_testing = pos_emb(X_testmod.shape[1],X_testmod.shape[2])\n",
    "print(position_enc_testing.shape)"
   ]
